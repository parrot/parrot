# [expr abs()]

.sub __math_abs
  .param pmc argv

  # requires 1 args, numeric only

  .local pmc result 
  result = new PerlArray

  # XXX assuming numeric

  # XXX also assuming 1 arg
  pop $P0, argv

  $I0 = $P0[1]

  abs $I1, $I0

  result[0] = INTEGER
  result[1] = $I1

  push argv, result

  .pcc_begin_return
  .return argv
  .return INTEGER
  .pcc_end_return   
.end
